{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vacs-client",
  "version": "0.1.0",
  "identifier": "com.vacs-client",
  "build": {
    "beforeDevCommand": "npm --prefix frontend run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm --prefix frontend run build",
    "frontendDist": "./frontend/dist"
  },
  "app": {
    "windows": [
      {
        "title": "vacs",
        "width": 1030,
        "height": 780
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "rpm",
      "msi",
      "nsis",
      "app",
      "dmg"
    ],
    "icon": [
      "frontend/icons/32x32.png",
      "frontend/icons/128x128.png",
      "frontend/icons/128x128@2x.png",
      "frontend/icons/icon.icns",
      "frontend/icons/icon.ico"
    ]
  }
}
